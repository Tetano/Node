<!DOCTYPE html>
<html lang="pt-br">

<head>
	<meta charset="utf-8" />
	<link rel="stylesheet" type="text/css" href="/css/noticias.css">
	<title>Notícias</title>
</head>

<body>

	<h1 class="classe">Notícias</h1>
	<table>
		<tr>
			<td>ID</td>
			<td>Titulo</td>
			<td>Noticia</td>
			<td>Data Da noticia</td>
		</tr>
		<% for(var i=0; i< noticias.length; i++) { %>
			
		<tr>
			<td><%= noticias[i].id_noticia %></td>
			<td><%= noticias[i].titulo%></td>
			<td><%= noticias[i].noticia%></td>
			<td><%= noticias[i].data_criacao%></td>
		</tr>
	<% } %>
	</table>
</body>

</html>